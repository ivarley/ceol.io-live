{
  "name": "ceol-io",
  "version": "1.0.0",
  "description": "Traditional Irish music session tracking web application",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "type-check": "tsc --noEmit",
    "start": "npm run build && flask --app app run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.1",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "jsdom": "^26.1.0",
    "ts-jest": "^29.4.1"
  }
}
